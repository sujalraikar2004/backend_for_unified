{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "EMAIL_USER": "@email_user",
    "EMAIL_PASS": "@email_pass",
    "PORT": "4500"
  }
}
